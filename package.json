{
  "name": "micro-yff-programomrader",
  "description": "microservice for yff programomrader",
  "version": "2.1.5",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "update-vg3": "node utils/yff-generate-programomraader.js vg3",
    "update-vg2": "node utils/yff-generate-programomraader.js vg2",
    "update-vg1": "node utils/yff-generate-programomraader.js vg1",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "yff",
    "minelev",
    "louie",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-yff-programomrader.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-yff-programomrader/issues"
  },
  "homepage": "https://github.com/telemark/micro-yff-programomrader#readme",
  "now": {
    "alias": [
      "yff.service.minelev.no"
    ],
    "type": "npm",
    "dotenv": "production.env",
    "engines": {
      "node": "10.0.0"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "axios": "0.18.0",
    "cheerio": "1.0.0-rc.2",
    "coveralls": "3.0.2",
    "micro-dev": "3.0.0",
    "nsp": "3.2.1",
    "nyc": "13.0.1",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "cors": "2.8.4",
    "finalhandler": "1.1.1",
    "markdown-it": "8.4.2",
    "micro": "9.3.3",
    "router": "1.3.3",
    "winston": "2.4.4",
    "winston-papertrail": "1.0.5"
  }
}
